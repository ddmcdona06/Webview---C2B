{
  "expo": {
    "name": "connect2black-webview",
    "slug": "connect2black-webview",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/c2b-icon.png",
    "scheme": "connect2blackwebview",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.ddmcdona06.connect2blackwebview",
      "supportsTablet": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "permissions": [],
      "package": "com.ddmcdona06.connect2blackwebview",
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/c2b-icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/c2b-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e3dffbbc-9b14-4477-ba13-a0dfb609e8a8"
      }
    }
  }
}
